<h1>Patient Show Page: </h1>

<%= @patient.name %>
<br>
<%= @patient.age%>
<br>
<ol>
<% @patient.appointments.each do |appointment|%>
    <li><%= appointment.appointment_datetime.strftime("%B %d, %Y at %H:%M")  %> - <%= link_to appointment.doctor.name, "/doctors/#{appointment.doctor.id}" %>  </li>
<%end%>
</ol>